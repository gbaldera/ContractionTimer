<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.ianhanniballake.contractiontimer"
	android:installLocation="auto"
	android:versionCode="1314121"
	android:versionName="1.2.1">
	<uses-sdk
		android:minSdkVersion="4"
		android:targetSdkVersion="13" />
	<uses-permission
		android:name="android.permission.INTERNET" />
	<uses-permission
		android:name="android.permission.ACCESS_NETWORK_STATE" />
	<application
		android:icon="@drawable/icon"
		android:theme="@style/Theme"
		android:label="@string/app_name"
		android:debuggable="true">
		<provider
			android:name=".provider.ContractionProvider"
			android:authorities="com.ianhanniballake.contractiontimer"
			android:exported="false">
			<grant-uri-permission
				android:pathPattern=".*" />
		</provider>
		<activity
			android:name=".ui.MainActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action
					android:name="android.intent.action.MAIN" />
				<category
					android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
        <service
            android:name=".service.AnalyticsService" />
	</application>
</manifest>